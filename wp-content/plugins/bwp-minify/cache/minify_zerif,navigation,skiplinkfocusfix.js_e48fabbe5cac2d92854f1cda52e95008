jQuery(document).ready(function(){if(jQuery(window).width()<767){jQuery('#site-navigation li').each(function(){if(jQuery(this).find('ul').length>0){jQuery(this).addClass('has_children');jQuery(this).find('a').first().after('<p class="dropdownmenu"></p>');}});}
